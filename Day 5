import java.io.*;
import java.math.*;
import java.security.*;
import java.text.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) throws IOException {
     Scanner sc= new Scanner(System.in);
     int n=sc.nextInt();
     for(int i=1;i<=10;i++){
        System.out.println(n+" x "+i+" = "+ n*i);     
     }
    

    }
}
